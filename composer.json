{
  "name": "aqwelai/larai",
  "description": "LarAI - AI integration package for Laravel developed by Aqwel AI",
  "type": "library",
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^11.0",
    "illuminate/contracts": "^11.0",
    "illuminate/bus": "^11.0",
    "illuminate/queue": "^11.0",
    "illuminate/http": "^11.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "laravel/framework": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "AqwelAI\\LarAI\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AqwelAI\\LarAI\\LarAIServiceProvider"
      ],
      "aliases": {
        "LarAI": "AqwelAI\\LarAI\\Facades\\LarAI",
        "AI": "AqwelAI\\LarAI\\Facades\\AI"
      }
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:Aqwel-AI/larai.git"
    }
  ]
}